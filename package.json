{
	"name": "saas-onboarding",
	"version": "0.1.0",
	"bin": {
		"saas-onboarding": "bin/saas-onboarding.js"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "LOCALSTACK_ENDPOINT=http://127.0.0.1:4566 GLOBAL_TABLE=global CONFIG_SET_NAME=test URL_PREFIX=http://localhost USERPOOL=test AWS_REGION=us-east-1 jest",
		"lint": "eslint . --ext .ts --fix",
		"postinstall": "husky install",
		"localstack:start": "localstack start -d",
		"localstack:stop": "localstack stop",
		"cdk": "cdk",
		"config:dev": "cp ./config/config.dev.json ./config/config.json",
		"config:staging": "cp ./config/config.staging.json ./config/config.json",
		"config:prod": "cp ./config/config.prod.json ./config/config.json",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.5",
		"@commitlint/config-conventional": "^17.6.5",
		"@faker-js/faker": "^8.0.2",
		"@swc/core": "^1.3.66",
		"@swc/jest": "^0.2.26",
		"@types/aws-lambda": "^8.10.117",
		"@types/bcryptjs": "^2.4.2",
		"@types/hapi__joi": "^17.1.9",
		"@types/jest": "^29.4.0",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/nanoid": "^3.0.0",
		"@types/node": "18.14.6",
		"@types/pino": "^7.0.5",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"aws-cdk": "2.77.0",
		"aws-lambda": "^1.0.7",
		"aws-sdk": "^2.1403.0",
		"commitlint": "^17.6.5",
		"eslint": "^8.41.0",
		"eslint-config-standard-with-typescript": "^34.0.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^16.0.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-standard": "^5.0.0",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"jest-mock-extended": "^3.0.4",
		"lint-staged": "^13.2.2",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"typescript": "~4.9.5"
	},
	"dependencies": {
		"@hapi/joi": "^17.1.1",
		"aws-cdk-lib": "2.77.0",
		"bcryptjs": "^2.4.3",
		"constructs": "^10.0.0",
		"dayjs": "^1.11.8",
		"json-schema-to-ts": "^2.9.1",
		"jsonwebtoken": "^9.0.0",
		"nanoid": "^4.0.2",
		"pino": "^8.14.1",
		"pino-pretty": "^10.0.0",
		"source-map-support": "^0.5.21",
		"uuid": "^9.0.0"
	},
	"lint-staged": {
		"*.ts": "eslint --cache --fix"
	}
}
